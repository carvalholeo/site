# DO NOT REMOVE THIS LINE AND THE LINES BELOW SSL_REDIRECT:leocarvalho.tech
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} ^leocarvalho.tech$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE SSL_REDIRECT:leocarvalho.tech

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ index.php [L]

<filesMatch "\.(txt|html|js|css|htm)$">
ExpiresDefault A7200
Header append Cache-Control "proxy-revalidate"
</filesMatch>